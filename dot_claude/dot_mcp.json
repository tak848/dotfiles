{
   "mcpServers": {
      "gemini-cli": {
         "args": [
            "@choplin/mcp-gemini-cli",
            "--allow-npx"
         ],
         "command": "npx",
         "env": { },
         "type": "stdio"
      },
      "o3": {
         "args": [
            "o3-search-mcp"
         ],
         "command": "npx",
         "env": {
            "REASONING_EFFORT": "medium",
            "SEARCH_CONTEXT_SIZE": "medium"
         }
      },
      "playwright": {
         "args": [
            "@playwright/mcp@latest"
         ],
         "command": "npx",
         "env": { },
         "type": "stdio"
      }
   }
}
